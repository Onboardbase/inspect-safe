{
  "verison":"1.0",
  "paths":{
    "/path:*":{
      "headers":[
        {
          "key":"content-security-policy",
          "value":"default-src 'self'"
        },
        {
          "key":"referrer-policy",
          "value":"origin"
        },
        {
          "key":"x-content-type-options",
          "value":"nosniff"
        }
      ]
    },
    "/api:*":{
      "headers":[
        {
          "key":"x-frame-options",
          "value":"DENY"
        },
        {
          "key":"x-xss-protection",
          "value":"1; mode=block"
        }
      ]
    }
  }
}